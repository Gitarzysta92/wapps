{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "@nx/js:tsc": {
      "cache": true,
      "inputs": ["default", "^default"]
    }
  },
  "projects": {},
  "generators": {
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "scss"
    }
  },
  "cli": {},
  "tasksRunnerOptions": {},
  "workspaceLayout": {},
  "defaultProject": "",
  "pluginsConfig": {},
  "enforceModuleBoundaries": [
    {
      "sourceTag": "type:app",
      "onlyDependOnLibsWithTags": [
        "type:lib",
        "type:ui",
        "type:primitive",
        "type:domain"
      ]
    },
    {
      "sourceTag": "type:domain",
      "onlyDependOnLibsWithTags": ["type:lib", "type:ui", "type:primitive"]
    },
    {
      "sourceTag": "type:lib",
      "onlyDependOnLibsWithTags": ["type:ui", "type:primitive"]
    },
    {
      "sourceTag": "type:ui",
      "onlyDependOnLibsWithTags": ["type:primitive"]
    }
  ],
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    }
  ]
}
