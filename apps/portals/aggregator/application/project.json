{
  "name": "aggregator-angular",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "app",
  "sourceRoot": "apps/portals/aggregator/application/src",
  "tags": ["type:lib"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/portals/aggregator/application",
        "main": "apps/portals/aggregator/application/src/index.ts",
        "tsConfig": "apps/portals/aggregator/application/tsconfig.lib.json",
        "assets": ["apps/portals/aggregator/application/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/portals/aggregator/application/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
