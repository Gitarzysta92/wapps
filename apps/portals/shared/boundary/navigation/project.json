{
  "name": "portals-shared-boundary-navigation",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "id",
  "sourceRoot": "apps/portals/shared/boundary/navigation",
  "tags": ["type:boundary", "scope:navigation"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/portals/shared/boundary/navigation",
        "main": "apps/portals/shared/boundary/navigation/src/index.ts",
        "tsConfig": "apps/portals/shared/boundary/navigation/tsconfig.lib.json",
        "assets": ["apps/portals/shared/boundary/navigation/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/portals/shared/boundary/navigation/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}

