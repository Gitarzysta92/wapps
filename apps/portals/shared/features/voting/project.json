{
  "name": "voting",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portals/shared/features/voting/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["type:feature", "scope:voting"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/portals/shared/features/voting",
        "main": "apps/portals/shared/features/voting/src/index.ts",
        "tsConfig": "apps/portals/shared/features/voting/tsconfig.lib.json",
        "assets": ["apps/portals/shared/features/voting/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/portals/shared/features/voting/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
