{
  "name": "health-status",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portals/shared/features/health-status/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["type:feature", "scope:health"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "apps/portals/shared/features/health-status/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "apps/portals/shared/features/health-status/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "apps/portals/shared/features/health-status/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/portals/shared/features/health-status/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
