{
  "name": "platform-portal-angular",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "platform",
  "sourceRoot": "apps/portals/platform-portal/application/src",
  "tags": ["type:lib"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "tsc -p apps/portals/platform-portal/application/tsconfig.app.json --noEmit"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/portals/platform-portal/application/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}

