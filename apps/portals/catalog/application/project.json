{
  "name": "catalog-angular",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "catalog",
  "sourceRoot": "apps/portals/catalog/application/src",
  "tags": ["type:lib"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/portals/catalog/application",
        "main": "apps/portals/catalog/application/src/index.ts",
        "tsConfig": "apps/portals/catalog/application/tsconfig.app.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/portals/catalog/application/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
