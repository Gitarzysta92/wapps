{
  "kind": "collectionType",
  "collectionName": "store_links",
  "info": {
    "singularName": "store-link",
    "pluralName": "store-links",
    "displayName": "Store Link"
  },
  "attributes": {
    "storeId": {
      "type": "enumeration",
      "enum": ["GooglePlay", "AppleStore", "AppGallery", "MicrosoftStore"],
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "appRecord": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::app-record.app-record",
      "inversedBy": "stores"
    }
  }
}
