{
  "name": "apps.services.editorial",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/services/editorial/src",
  "projectType": "application",
  "tags": ["type:app", "scope:editorial"],
  "targets": {
    "develop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi develop",
        "cwd": "apps/services/editorial"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi build",
        "cwd": "apps/services/editorial"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi start",
        "cwd": "apps/services/editorial"
      }
    }
  }
}