{
  "name": "apps.services.firebase-auth-validator",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/services/firebase-auth-validator/src",
  "projectType": "application",
  "tags": ["type:app", "scope:platform", "layer:auth"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/services/firebase-auth-validator",
        "main": "apps/services/firebase-auth-validator/src/main.ts",
        "tsConfig": "apps/services/firebase-auth-validator/tsconfig.app.json",
        "webpackConfig": "apps/services/firebase-auth-validator/webpack.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true
        },
        "production": {
          "optimization": true,
          "sourceMap": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "apps.services.firebase-auth-validator:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "apps.services.firebase-auth-validator:build:development"
        },
        "production": {
          "buildTarget": "apps.services.firebase-auth-validator:build:production"
        }
      }
    }
  }
}

