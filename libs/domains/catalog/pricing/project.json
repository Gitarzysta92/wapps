{
  "name": "catalog-pricing",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "cat",
  "sourceRoot": "libs/domains/catalog/pricing/src",
  "tags": [
    "type:domain",
    "scope:catalog"
  ],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/domains/catalog/pricing/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/../../dist/domains/catalog/pricing"
      ],
      "options": {
        "command": "tsc --build libs/domains/catalog/pricing/tsconfig.lib.json"
      }
    }
  }
}
