{
  "name": "catalog-tags",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "cat",
  "sourceRoot": "libs/domains/catalog/tags/src",
  "tags": [
    "type:domain",
    "scope:catalog"
  ],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/domains/catalog/tags/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/domains/catalog/tags",
        "main": "libs/domains/catalog/tags/src/index.ts",
        "tsConfig": "libs/domains/catalog/tags/tsconfig.lib.json",
        "assets": [
          "libs/domains/catalog/tags/*.md"
        ]
      }
    }
  }
}
