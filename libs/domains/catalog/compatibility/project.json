{
  "name": "catalog-compatibility",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "prefix": "cat",
  "sourceRoot": "libs/domains/catalog/compatibility/src",
  "tags": [
    "type:domain",
    "scope:catalog"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/domains/catalog/compatibility",
        "main": "libs/domains/catalog/compatibility/src/index.ts",
        "tsConfig": "libs/domains/catalog/compatibility/tsconfig.lib.json",
        "assets": [
          "libs/domains/catalog/compatibility/*.md"
        ]
      }
    }
  }
}
