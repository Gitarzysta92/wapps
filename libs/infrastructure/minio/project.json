        "outputPath": "dist/libs/infrastructure/minio",
        "main": "libs/infrastructure/minio/src/index.ts",
        "tsConfig": "libs/infrastructure/minio/tsconfig.lib.json",
        "assets": ["libs/infrastructure/minio/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/infrastructure/minio/jest.config.ts"
      }
    }
  }
}
